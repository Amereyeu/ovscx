{
  "arrow function": {
    "prefix": "js-arrow-function",
    "description": "Arrow Function",
    "body": [
      "($1) => {",
      "  $2",
      "}",
      "$0",
      ""
    ],
  },
  "react-dom": {
    "prefix": "react-browser-router",
    "description": "React Router Import",
    "body": [
      "import { BrowserRouter as Router, Switch, Route  } from 'react-router-dom';"
    ],
  },
  "react-footer": {
    "prefix": "react-footer",
    "description": "footer with svg and year",
    "body": [
      "function Footer() {",
      "  const today = new Date();",
      "",
      "  return (",
      "    <footer className=\"footer\">",
      "      <svg",
      "        xmlns=\"http://www.w3.org/2000/svg\"",
      "        viewBox=\"0 0 99.21 136.06\"",
      "        className=\"footer__logo\"",
      "      >",
      "        <path d=\"M4.414 135.336c-.176-.908-.395-1.809-.522-2.723-.642-4.645-1.377-9.279-1.849-13.939a87.485 87.485 0 0 1-.409-10.096c.048-3.143.351-6.287 1.583-9.266.799-1.93 2.265-3.107 \" />",
      "      </svg>",
      "",
      "      <div className=\"footer__copyright\">",
      "        <p>Copyright &copy;2018 - {today.getFullYear()} foo</p>",
      "      </div>",
      "    </footer>",
      "  );",
      "}",
      "",
      "export default Footer;",
      ""
    ]
  },
  "backtics": {
    "prefix": "js-backtics",
    "description": "JS Backticks",
    "body": [
      "`$1 ${$2} $3`$0"
    ],
  },
  "react-comment": {
    "prefix": "react-comment",
    "description": "comment in react",
    "body": [
      "/*********************************************",
      " * $1",
      " *********************************************/",
      "",
      "$0",
      ""
    ]
  },
  "react-classname": {
    "prefix": "react-conditional-classname",
    "description": "conditional classname in react",
    "body": [
      "{`$4 \\${ $1 ? \"$2\" : \"$3\"}`} $0",
    ]
  },
  "react-src": {
    "prefix": "react-src",
    "description": "src in react",
    "body": [
      "{`$1/${$2}`} $0",
    ]
  }
}

