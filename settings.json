  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "none",
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "top",
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emmet.excludeLanguages": ["markdown", "jade", "pug"],
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "afterDelay",
  "files.insertFinalNewline": true,
  "git.enableSmartCommit": true,
  "gitlens.views.branches.branches.layout": "list",
  "gitlens.views.repositories.files.layout": "tree",
  // "highlight-matching-tag.customEmptyElements": null,
  "highlight-matching-tag.highlightFromContent": true,
  "highlight-matching-tag.highlightSelfClosing": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "liveServer.settings.donotShowInfoMsg": true,
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.files.color": "#ffffff",
  "material-icon-theme.folders.color": "#ffffff",
  "material-icon-theme.saturation": 0,
  "MinifyAll.disableJavascriptReact": false,
  "notebook.showFoldingControls": "always",
  "php.validate.executablePath": "/usr/share/libgda-5.0/php",
  "prettier.bracketSameLine": true,
  "prettier.endOfLine": "crlf",
  "prettier.useTabs": false,
  "security.workspace.trust.untrustedFiles": "open",
  "ShortcutMenuBar.commentLine": true,
  "ShortcutMenuBar.navigateBack": false,
  "ShortcutMenuBar.navigateForward": false,
  "ShortcutMenuBar.openFilesList": false,
  "ShortcutMenuBar.redo": true,
  "ShortcutMenuBar.save": true,
  "ShortcutMenuBar.toggleTerminal": false,
  "ShortcutMenuBar.undo": true,
  "ShortcutMenuBar.userButton01Command": "markdown-editor.openEditor",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontWeight": "normal",
  "todo-tree.general.showIconsInsteadOfTagsInStatusBar": true,
  "todo-tree.general.statusBar": "tags",
  "todo-tree.general.statusBarClickBehaviour": "cycle",
  "todo-tree.regex.regex": "(//||#|<!--|;|//-|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS).*(\\n\\s*//\\s{2,}.*)*",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.cursorStylePerMode.normal": "block-outline",
  "vim.easymotion": true,
  "vim.easymotionDimColor": "#777",
  "vim.easymotionIncSearchForegroundColor": "#0bd3d4",
  "vim.easymotionMarkerForegroundColorOneChar": "#0bd3d4",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#0bd3d4",
  "vim.easymotionMarkerForegroundColorTwoCharSecond": "#0bd3d4",
  "vim.foldfix": true,
  "vim.highlightedyank.color": "#4f4f4f",
  "vim.highlightedyank.textColor": "#fff",
  "vim.highlightedyank.duration": 2000,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.ignorecase": true,
  "vim.leader": "<Space>",
  "vim.searchHighlightColor": "#0bd3d4",
  "vim.searchHighlightTextColor": "#fff",
  "vim.mouseSelectionGoesIntoVisualMode": false,
  "vim.showMarksInGutter": true,
  "vim.smartRelativeLine": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.commandlineinprogress": ["#12abe3", "#000000"],
  "vim.statusBarColors.easymotioninputmode": ["#12abe3", "#000000"],
  "vim.statusBarColors.easymotionmode": ["#12abe3", "#000000"],
  "vim.statusBarColors.insert": ["#D8DEE9", "#000000"],
  "vim.statusBarColors.normal": ["#000000", "#fbfbfb"],
  "vim.statusBarColors.replace": ["#89c0d0", "#000000"],
  "vim.statusBarColors.searchinprogressmode": ["#12abe3", "#000000"],
  "vim.statusBarColors.surroundinputmode": ["#12abe3", "#000000"],
  "vim.statusBarColors.visual": ["#D6AD60", "#000000"],
  "vim.statusBarColors.visualblock": ["#D6AD60", "#000000"],
  "vim.statusBarColors.visualline": ["#D6AD60", "#000000"],
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.vimrc.enable": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.menuBarVisibility": "toggle",
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "B2",
  "workbench.enableExperiments": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.productIconTheme": "icons-carbon",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 20,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.colorCustomizations": {
    "terminal.background": "#000",
    "terminal.foreground": "#fff",
    "statusBar.background": "#000000",
    "statusBar.noFolderBackground": "#000000",
    "statusBar.debuggingBackground": "#000000",
    "statusBar.foreground": "#fbfbfb",
    "statusBar.debuggingForeground": "#fbfbfb"
  },
  "emmet.includeLanguages": {
    "twig": "html",
    "html": "html",
    "javascript": "javascriptreact",
    "astro": "javascript"
  },
  "files.associations": {
    "*.html": "html",
    "*.js": "javascriptreact",
    "*.mdx": "markdown"
  },
  "files.exclude": {
    "**/.history": true
  },
  // "highlight-matching-tag.styles": {
  //   "opening": {
  //     "full": {
  //       "underline": "yellow"
  //     }
  //   }
  // },
  "indentRainbow.colors": [
    "rgba(255,255,255,0.1)",
    "rgba(255,255,255,0.2)",
    "rgba(255,255,255,0.3)",
    "rgba(255,255,255,0.2)",
    "rgba(255,255,255,0.1)"
  ],
  "todo-tree.general.tags": ["todo:", "bug:", "fixme:", "done:", "note:"],
  "todo-tree.filtering.excludeGlobs": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/.vscode/**",
    "**/.github/**",
    "**/*.min.*",
    "**/*.map",
    "**/*.yaml"
  ],
  "todo-tree.highlights.defaultHighlight": {
    "background": "#ffffff",
    "foreground": "#000000",
    "gutterIcon": true,
    "icon": "alert",
    "iconColour": "#ffffff",
    "type": "whole-line"
  },
  "todo-tree.highlights.customHighlight": {
    "todo:": {
      "background": "#F1A104",
      "foreground": "#FFFFFF",
      "gutterIcon": true,
      "icon": "note",
      "iconColour": "#F1A104",
      "type": "whole-line"
    },
    "bug:": {
      "background": "#F94238",
      "foreground": "#FFFFFF",
      "gutterIcon": true,
      "icon": "bug",
      "iconColour": "#F94238",
      "type": "whole-line"
    },
    "fixme:": {
      "background": "#4B606F",
      "foreground": "#FFFFFF",
      "gutterIcon": true,
      "icon": "zap",
      "iconColour": "#4B606F",
      "type": "whole-line"
    },
    "done:": {
      "background": "#25B396",
      "foreground": "#FFFFFF",
      "gutterIcon": true,
      "icon": "verified",
      "iconColour": "#25B396",
      "type": "whole-line"
    },
    "note:": {
      "background": "#1669a4",
      "foreground": "#FFFFFF",
      "gutterIcon": true,
      "icon": "pencil",
      "iconColour": "#1669a4",
      "type": "whole-line"
    }
  },
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": true,
      "other": true,
      "strings": true
    }
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<Leader>", "u"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<Leader>", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<Leader>", "t"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<Leader>", "n"],
      "commands": ["extension.advancedNewFile"]
    },
    {
      "before": ["<Leader>", "o"],
      "commands": [
        "workbench.action.toggleSidebarVisibility",
        "workbench.action.toggleActivityBarVisibility"
      ]
    },
    {
      "before": ["<Leader>", "e"],
      "commands": ["workbench.action.focusSideBar"]
    },
    {
      "before": ["<tab>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<S-tab>"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<Leader>", "c"],
      "commands": ["workbench.action.closeActiveEditor"]
    }
  ]
}

