{
  "auto-close-tag.insertSpaceBeforeSelfClosingTag": true,
  "auto-close-tag.SublimeText3Mode": true,
  "breadcrumbs.enabled": false,
  "color-highlight.languages": ["*"],
  "color-highlight.markerType": "underline",
  "color-highlight.markRuler": false,
  "dashboard.openOnStartup": "never",
  "dashboard.prependVscodeUrlToWslRemotes": false,
  "dashboard.projectTileWidth": 260,
  "dashboard.recentColorsToRemember": 0,
  "dashboard.searchIsActiveByDefault": false,
  "editor.autoClosingOvertype": "auto",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 5,
  "editor.detectIndentation": false,
  "editor.fontFamily": "'JetBrains Mono','Fira Code', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.fontWeight": "400",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.hover.enabled": false,
  "editor.indentSize": "tabSize",
  "editor.letterSpacing": 0.5,
  "editor.lineHeight": 22,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
